<h3>{{t 'auth.users.sign_in'}}</h3>
<hr/>
<form {{action "signIn" on="submit"}}>
  <div class="form-group">
    <label>{{t 'auth.attributes.user.email'}}</label>
    {{input type="email" value=email class="form-control"}}
  </div>
  <div class="form-group">
    <label>{{t 'auth.attributes.user.password'}}</label>
    {{input type="password" value=password class="form-control"}}
  </div>
  <button type="submit" class="btn btn-primary">{{t 'buttons.submit'}}</button>
</form>
